{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/indexify-server/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/indexify-server/tsconfig.app.json"
  },
  "projects": {
    "indexify-server": {
      "type": "application",
      "root": "apps/indexify-server",
      "entryFile": "main",
      "sourceRoot": "apps/indexify-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/indexify-server/tsconfig.app.json"
      }
    },
    "typesense": {
      "type": "library",
      "root": "libs/typesense",
      "entryFile": "index",
      "sourceRoot": "libs/typesense/src",
      "compilerOptions": {
        "tsConfigPath": "libs/typesense/tsconfig.lib.json"
      }
    },
    "error-exception": {
      "type": "library",
      "root": "libs/error-exception",
      "entryFile": "index",
      "sourceRoot": "libs/error-exception/src",
      "compilerOptions": {
        "tsConfigPath": "libs/error-exception/tsconfig.lib.json"
      }
    },
    "common-utility": {
      "type": "library",
      "root": "libs/common-utility",
      "entryFile": "index",
      "sourceRoot": "libs/common-utility/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common-utility/tsconfig.lib.json"
      }
    },
    "send-mailer": {
      "type": "library",
      "root": "libs/send-mailer",
      "entryFile": "index",
      "sourceRoot": "libs/send-mailer/src",
      "compilerOptions": {
        "tsConfigPath": "libs/send-mailer/tsconfig.lib.json"
      }
    },
    "postgres.config": {
      "type": "library",
      "root": "libs/postgres.config",
      "entryFile": "index",
      "sourceRoot": "libs/postgres.config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/postgres.config/tsconfig.lib.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/indexify-server"
}
